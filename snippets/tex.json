{
    "Figure in the place": {
        "prefix": "figh",
        "body": [
            "\\begin{figure}[H]",
            "\t\\begin{center}",
            "\t\t\\includegraphics[width=0.95\\textwidth]{figures/$1}",
            "\t\\end{center}",
            "\t\\caption{$2.}",
            "\t\\label{fig:$3}",
            "\\end{figure}",
            "$0"
        ]
    },

    "Space between paragraphs": {
        "prefix": "sbp",
        "body": ["\\vspace{5mm}"]
    },
    "Figura doble en linea": {
        "prefix": "doublefig",
        "body": [
            "\\begin{figure}[!htb]",
            "\t\\begin{center}",
            "\t\t\\subfigure[$5.]{",
            "\t\t\t\\includegraphics[width=0.45\\textwidth]{figures/$1}",
            "\t\t\t\\label{$4:sub1}}",
            "\t\t\\subfigure[$6.]{",
            "\t\t\t\\includegraphics[width=0.45\\textwidth]{figures/$2}",
            "\t\t\t\\label{$4:sub2}}",
            "\t\t\\caption{$3.}",
            "\t\t\\label{fig:$4}",
            "\t\\end{center}",
            "\\end{figure}"
        ],
        "description": "Inserta dos figuras en linea"
    }
}
